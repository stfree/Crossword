    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.4.5",
    "react-router-dom": "^6.4.5"
  },
