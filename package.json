{
  "name": "capstone",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "concurrently --names \"Client,Server\" -c \"green.bold,magenta\" \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=8192 webpack --mode development --watch",
    "dev:server": "cross-env NODE_ENV=development nodemon --exec 'babel-node --inspect ./src/server/index.js'",
    "build": "webpack --mode production",
    "test": "jest",
    "mtest": "npx majestic",
    "smtest": "concurrently --names \"Storybook,Majestic\" -c \"green.bold,magenta\" \"yarn storybook\" \"yarn mtest\"",
    "prettier": "prettier --write .",
    "lint": "eslint src/client",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/computers-club/capstone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/computers-club/capstone/issues"
  },
  "homepage": "https://github.com/computers-club/capstone#readme",
