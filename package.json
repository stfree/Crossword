        "jest": "^28.1.1",
        "postcss": "^8.4.14",
        "postcss-loader": "^7.0.0",
        "postcss-preset-env": "^7.7.1",
        "prettier": "2.6.2",
        "react-test-renderer": "^18.1.0",
